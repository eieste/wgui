{% extends "base.jinja2" %}

{% block content_inner %}
    <h3 class="text-gray-700 text-3xl font-medium">Create new Tunnel Endpoint</h3>
    <div class="mt-4">
        <div class="flex flex-wrap -mx-6">
            <div class="w-full px-6 sm:w-1/2 xl:w-1/3">
                <form action="{{ url_for("tunnel_create") }}" method="POST">
                    <div class="mb-6">
                        <label for="email"
                               class="block mb-2 text-sm font-medium dark:text-gray-300">Device</label>
                        {{ form.device }}
                    </div>
                    <button type="submit"
                            class="inline-flex justify-center py-2 px-4 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500">
                        Save
                    </button>


                </form>
            </div>
        </div>


    </div>



{% endblock %}
